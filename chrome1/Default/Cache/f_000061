            <span class=\"primary--text title\">Danh sách công việc</span>\n                    <v-icon @click=\"listWorkShow = !listWorkShow\" color=\"primary\" class=\"float-right\" medium>{{\n                        listWorkShow ? 'mdi-arrow-down-bold-circle-outline' : 'mdi-arrow-right-bold-circle-outline' }}\n                    </v-icon>\n                    <v-icon @click=\"initialize()\" color=\"primary\" class=\"float-right\" medium>\n                        mdi-reload\n                    </v-icon>\n                </v-col>\n            </v-row>\n\n            <v-row v-if=\"listWorkShow && accounts.length !== 0\">\n                <v-col cols=\"12\" v-for=\"(job, key) in jobs10\" :key=\"job.id\">\n                    <router-link @click.native=\"scrollToTop\" v-if=\"key < 10\" :to=\"{ name : 'jobs_instagram_detail', params : { id : job.id } }\">\n                        <v-row class=\"job\">\n                            <v-col cols=\"3\" md=\"1\" class=\"text-center pa\">\n                                <div style=\"border-right: 1px solid #777777\">\n                                    <v-avatar size=\"36\">\n                                        <img :src=\"job.icon\" alt=\"\">\n                                    </v-avatar>\n                                    <h6 class=\"body-1 font-weight-bold mt-1\">\n                                        <span><span class=\"hold-prices\">{{ job.coin }}</span></span>\n                                        <font-awesome-icon :icon=\"['fas', 'coins']\" class=\"v-coin-icon white--text\"/>\n<!--                                        <img style=\"max-width: 18px\" :src=\"coinSvg\" alt=\"\">-->\n                                    </h6>\n                                </div>\n                            </v-col>\n                            <v-col cols=\"9\" md=\"11\" class=\"caption job-info px-0\" >\n                                <div class=\"hidden-md-and-up\">\n                                    <span class=\"nowrap font-weight-bold caption\">{{ job.seeding_title }}</span><br/>\n                                    <span class=\"black--text\">Còn lại <span class=\"font-weight-bold\">{{ job.num_seeding_remain }}</span><span class=\"float-right\">{{ job.time_remain }}<v-icon\n                                            small>mdi-arrow-right</v-icon></span></span><br/>\n                                    <span class=\"black--text\">{{ job.content | jobContentFormat}}</span>\n                                </div>\n                                <div class=\"hidden-sm-and-down\">\n                                    <span class=\"nowrap font-weight-bold title\">{{ job.seeding_title }}</span><br/>\n                                    <span class=\"black--text\">Còn lại <span class=\"font-weight-bold\">{{ job.num_seeding_remain }}</span><span class=\"float-right\">{{ job.time_remain }}<v-icon\n                                            small>mdi-arrow-right</v-icon></span></span><br/>\n                                    <span class=\"black--text\">{{ job.content }}</span>\n                                </div>\n                            </v-col>\n                        </v-row>\n                    </router-link>\n                </v-col>\n            </v-row>\n        </div>\n        <v-overlay :value=\"overlay\">\n            <v-progress-circular\n                    indeterminate\n                    color=\"info\"\n            ></v-progress-circular>\n        </v-overlay>\n    </v-card>\n</template>\n\n<script>\n    import {\n      LIST_SEEDING,\n    } from \"~/store/actions.type\";\n    import VueRecaptcha from 'vue-recaptcha';\n    import {JOB_TYPE_INSTAGRAM, GOOGLE_RECAPTCHA_KEY} from \"~/config/constants\";\n    import {\n      INSTAGRAM_ACCOUNT_FETCH_ALL,\n      // INSTAGRAM_JOB_FETCH_ALL,\n      INSTAGRAM_JOB_FETCH_ALL_V2\n    } from \"~/store/actions.type\";\n    import {COMMON_SET_SNACK} from \"~/store/actions.type\";\n    import moment from \"moment\";\n    global.jQuery = require('jquery');\n    let $ = global.jQuery;\n    window.$ = $;\n    export default {\n      components: { VueRecaptcha },\n        data() {\n            return {\n                lowPayJobs: false,\n                seeding: ['Like instagram', 'Tăng follow instagram', 'Bình luận instagram'],\n                coinSvg: '/images/coin.svg',\n                likePageGif: '/images/like_page.gif',\n                listAccountShow: false,\n                listWorkShow: true,\n                params : {'seeding_type' : 'all', 'instagram_account_id' : localStorage.getItem('instagram_account'), 'per_page' : 100, 'page' : 1, 'recaptcha': ''},\n                account: null,\n                jobs : {},\n                accounts : {},\n                listSeedingInstagram : [],\n                listSeedingOriginal : [],\n                profilePic : '',\n                accountName : '',\n                accountSelectedId : localStorage.getItem('instagram_account'),\n                jobs10 : {},\n                overlay: false,\n                g_recaptcha_key : GOOGLE_RECAPTCHA_KEY,\n                captchaVisible: false,\n            }\n        },\n        filters: {\n            jobContentFormat(content){\n                let cutoff = 30;\n                if(typeof content !== \"undefined\" && content.length > 30){\n                    return content.substring(0, cutoff)+'...';\n                }\n                return content;\n            }\n        },\n        computed: {\n\n        },\n        watch: {\n            listSeedingInstagram: {\n                handler(val){\n                    this.$store.dispatch('setListSeedingInstagram', this.listSeedingInstagram);\n                    let keys = [];\n                    console.log('asdasd', val, this.listSeedingOriginal);\n                    for(let k in val){\n                        for (let l in this.listSeedingOriginal){\n                            if(val[k] === this.listSeedingOriginal[l].title){\n                                keys.push(this.listSeedingOriginal[l].id);\n                            }\n                            // if(val[k] === this.capitalize(this.listSeedingOriginal[l].key.split('_').join(' '))){\n                            //     keys.push(this.listSeedingOriginal[l].id);\n                            // }\n                        }\n                    }\n                    this.params = { 'seeding_type' : keys.join(','), 'instagram_account_id' : this.accountSelectedId, 'per_page' : 100, 'page' : 1};\n                },\n                deep: true\n            }\n        },\n        created() {\n            this.instagramJobListSeedingFetchAll();\n            this.instagramAccountFetchAll();\n        },\n        methods: {\n            async isCacheJobsBlank(cacheJobKey) {\n              let cachedObj = await localStorage.getItem(cacheJobKey);\n              cachedObj = await JSON.parse(cachedObj)\n              if(!cachedObj || (cachedObj && cachedObj.jobs && cachedObj.jobs.length == 0) || (cachedObj && !cachedObj.jobs)) {\n                return null;\n              }else if (cachedObj && cachedObj.timeAt && moment().diff(moment(cachedObj.timeAt), 'minutes') >= 4){\n                return null\n              } else {\n                return cachedObj;\n              }\n            },\n            async initialize() {\n              this.jobs10 = [];\n              if(this.params.instagram_account_id){\n                const cached_account_id = await localStorage.getItem('instagram_account');\n                const instagram_account_id = cached_account_id ? cached_account_id : this.params.instagram_account_id;\n                this.params.instagram_account_id = instagram_account_id;\n                const cacheJobKey = instagram_account_id.toString() + '_cached_jobs';\n                this.overlay = true;\n                const cachedData = await this.isCacheJobsBlank(cacheJobKey);\n                if (!cachedData) {\n                  if (this.captchaVisible && this.recaptcha != '') {\n                    this.jobs = await this.$store.dispatch(INSTAGRAM_JOB_FETCH_ALL_V2, this.params);\n                    if (this.jobs) {\n                      const cacheObj = {\n                        timeAt: moment(),\n                        jobs: this.jobs\n                      }\n                      await localStorage.setItem(cacheJobKey, JSON.stringify(cacheObj))\n                    }\n                  } else {\n                    this.captchaVisible = true;\n                    this.overlay = false;\n                    this.$store.dispatch(COMMON_SET_SNACK, {\n                      message: 'Bạn cần thực hiện captcha để lấy jobs nhé!',\n                      type: \"error\"\n                    });\n                  }\n                } else {\n                  this.jobs = cachedData.jobs;\n                }\n                if (this.jobs) {\n                  this.jobs10 = this.jobs.slice(0, 20).map(i => {\n                    return i;\n                  });\n                  this.captchaVisible = false;\n                }\n                this.overlay = false;\n              }\n             },\n            async instagramJobListSeedingFetchAll(){\n                let listSeedingInstagram = await this.$store.dispatch(LIST_SEEDING, {'job_type' : JOB_TYPE_INSTAGRAM});\n                this.listSeedingOriginal = listSeedingInstagram;\n                console.log('aaa', this.$store.getters.listSeedingInstagram);\n                if((this.$store.getters.listSeedingInstagram).length > 0){\n                    this.listSeedingInstagram = this.$store.getters.listSeedingInstagram;\n                }else{\n                    for (let i in listSeedingInstagram){\n                        let key = (listSeedingInstagram[i].key).split('_').join(' ');\n                        key = this.capitalize(key);\n                        this.listSeedingInstagram.push(listSeedingInstagram[i].title);\n                    }\n                    this.$store.dispatch('setListSeedingInstagram', this.listSeedingInstagram);\n                }\n            },\n            async instagramAccountFetchAll() {\n                 await this.$store.dispatch(INSTAGRAM_ACCOUNT_FETCH_ALL)\n                    .then(({data}) => {\n                        this.accounts = data;\n                        if(data.length === 0) return;\n                        let main_account = {};\n                        if(!localStorage.getItem('instagram_account')) {\n                          main_account = data.filter(o => o.is_main == 1)[0];\n                        }else{\n                          main_account = data.filter(o => o.id == localStorage.getItem('instagram_account'))[0];\n                        }\n                        this.profilePic = main_account.profile_pic_url;\n                        this.accountName = main_account.username;\n                        this.accountSelectedId = main_account.id;\n                        this.params.instagram_account_id = main_account.id;\n                        localStorage.setItem('instagram_account', main_account.id);\n                        this.initialize();\n                    });\n            },\n            accountSelected(account) {\n                this.profilePic = account.profile_pic_url;\n                this.accountName = account.username;\n                this.accountSelectedId = account.id;\n                localStorage.setItem('instagram_account', account.id);\n                this.listAccountShow = false;\n                this.params.instagram_account_id = account.id;\n                this.initialize();\n            },\n            getRandomJobs(){\n                let jobs = this.jobs;\n                const shuffled = jobs.sort(() => 0.5 - Math.random());\n                this.jobs10 = shuffled.slice(0, 10);\n                this.$store.dispatch(COMMON_SET_SNACK, {\n                    message: 'Lấy jobs thành công !',\n                    type: \"success\"\n                });\n            },\n            capitalize(a){\n                return a.replace(/(?:^|\\s)\\S/g, function(a) { return a.toUpperCase(); });\n            },\n            remove(item) {\n                this.listSeedingInstagram.splice(this.listSeedingInstagram.indexOf(item), 1);\n                this.listSeedingInstagram = [...this.listSeedingInstagram]\n            },\n            scrollToTop() {\n                window.scrollTo(0,0);\n            },\n            onVerify(response) {\n              this.recaptcha = response;\n              this.params.recaptcha = response;\n              this.initialize();\n            },\n            resetRecaptcha: function() {\n              this.grecaptcha.reset();\n            },\n        }\n    };\n</script>\n<style scoped lang=\"scss\">\n    .work-list {\n        .job {\n            .v-avatar{\n               height: 40px !important;\n            }\n            background-color: #ffffff;\n            cursor: pointer;\n            .job-info {\n                position: relative;\n                div {\n                    position: absolute;\n                    width: 100%;\n                    top: 50%;\n                    transform: translateY(-50%);\n                }\n                .goto-detail {\n                    text-decoration: none;\n                    color: unset;\n                }\n            }\n\n        }\n\n        .v-chip {\n            &.share {\n                background: linear-gradient(69.69deg, #00F8BC 9.02%, #58C2F1 90.81%);\n            }\n        }\n\n        a {\n            text-decoration: none;\n        }\n    }\n    .recaptcha {\n      position: relative;\n      padding-top: 70px;\n      padding-bottom: 55px;\n      .g-recaptcha {\n        position: absolute;\n        left: 50%;\n        margin-left: -152px;\n      }\n    }\n\n</style>\n","import mod from \"-!../../../../node_modules/mini-css-extract-plugin/dist/loader.js??ref--8-oneOf-1-0!.