iginal : [],\n                profilePic : '',\n                accountName : '',\n                accountSelectedId : localStorage.getItem('tiktok_account'),\n                jobs10 : {},\n                overlay: false,\n                g_recaptcha_key : GOOGLE_RECAPTCHA_KEY,\n                captchaVisible: false,\n            }\n        },\n        filters: {\n          endDay(val) {\n            let start = moment(new Date());\n            let end = moment(val,'YYYY-MM-DD');\n            return end.diff(start, 'days');\n          },\n            jobContentFormat(content){\n                let cutoff = 30;\n                if(typeof content !== \"undefined\" && content.length > 30){\n                    return content.substring(0, cutoff)+'...';\n                }\n                return content;\n            }\n        },\n        computed: {\n            lowPayJobsStatus() {\n                return this.lowPayJobs ? 'Bật' : 'Tắt';\n            },\n            tiktokAccountSelected(){\n                return localStorage.getItem('tiktok_account');\n            }\n        },\n      watch: {\n        listSeedingTiktok: {\n          handler(val){\n            this.$store.dispatch('setListSeedingTiktok', this.listSeedingTiktok);\n            let keys = [];\n            console.log('zxc', val, this.listSeedingOriginal);\n            for(let k in val){\n              for (let l in this.listSeedingOriginal){\n                if(val[k] === this.listSeedingOriginal[l].title){\n                  keys.push(this.listSeedingOriginal[l].id);\n                }\n                // if(val[k] === this.capitalize(this.listSeedingOriginal[l].key.split('_').join(' '))){\n                //     keys.push(this.listSeedingOriginal[l].id);\n                // }\n              }\n            }\n            this.params = { 'seeding_type' : keys.join(','), 'tiktok_account_id' : this.accountSelectedId, 'per_page' : 100, 'page' : 1};\n          },\n          deep: true\n        }\n      },\n        created() {\n        this.tiktokJobListSeedingFetchAll();\n        this.tiktokAccountFetchAll();\n      },\n        methods: {\n          async isCacheJobsBlank(cacheJobKey) {\n            let cachedObj = await localStorage.getItem(cacheJobKey);\n            cachedObj = await JSON.parse(cachedObj)\n            if(!cachedObj || (cachedObj && cachedObj.jobs && cachedObj.jobs.length == 0) || (cachedObj && !cachedObj.jobs)) {\n              return null;\n            }else if (cachedObj && cachedObj.timeAt && moment().diff(moment(cachedObj.timeAt), 'minutes') >= 4){\n              return null\n            } else {\n              return cachedObj;\n            }\n          },\n          async initialize() {\n            this.jobs10 = [];\n            if(this.params.tiktok_account_id){\n              const cached_account_id = await localStorage.getItem('tiktok_account');\n              const tiktok_account_id = cached_account_id ? cached_account_id : this.params.tiktok_account_id;\n              this.params.tiktok_account_id = tiktok_account_id;\n              const cacheJobKey = tiktok_account_id.toString() + '_cached_jobs';\n              const cachedData = await this.isCacheJobsBlank(cacheJobKey);\n              this.overlay = true;\n              if (!cachedData) {\n                if (this.captchaVisible && this.recaptcha != '') {\n                this.jobs = await this.$store.dispatch(TIKTOK_JOB_FETCH_ALL_V2, this.params);\n                if (this.jobs) {\n                  const cacheObj = {\n                    timeAt: moment(),\n                    jobs: this.jobs\n                  }\n                  await localStorage.setItem(cacheJobKey, JSON.stringify(cacheObj))\n                }\n                } else {\n                  this.captchaVisible = true;\n                  this.overlay = false;\n                  this.$store.dispatch(COMMON_SET_SNACK, {\n                    message: 'Bạn cần thực hiện captcha để lấy jobs nhé!',\n                    type: \"error\"\n                  });\n                }\n              } else {\n                this.jobs = cachedData.jobs;\n              }\n              if (this.jobs) {\n                this.jobs10 = this.jobs.slice(0, 20).map(i => {\n                  return i;\n                });\n                this.captchaVisible = false;\n              }\n              this.overlay = false;\n            }\n           },\n          async tiktokJobListSeedingFetchAll(){\n            let listSeedingTiktok = await this.$store.dispatch(LIST_SEEDING, {'job_type' : JOB_TYPE_TIKTOK});\n            this.listSeedingOriginal = listSeedingTiktok;\n            if((this.$store.getters.listSeedingTiktok).length > 0){\n              this.listSeedingTiktok = this.$store.getters.listSeedingTiktok;\n            }else{\n              for (let i in listSeedingTiktok){\n                let key = (listSeedingTiktok[i].key).split('_').join(' ');\n                key = this.capitalize(key);\n                this.listSeedingTiktok.push(listSeedingTiktok[i].title);\n              }\n              this.$store.dispatch('setListSeedingTiktok', this.listSeedingTiktok);\n            }\n          },\n\n            //lay danh sach account\n            async tiktokAccountFetchAll() {\n                 await this.$store.dispatch(TIKTOK_ACCOUNT_FETCH_ALL)\n                    .then(({data}) => {\n                        this.accounts = data;\n                        if(data.length === 0) return;\n                        let main_account = {};\n                        if(!localStorage.getItem('tiktok_account')) {\n                            main_account = data.filter(o => o.is_main == 1)[0];\n                        }else{\n                            main_account = data.filter(o => o.id == localStorage.getItem('tiktok_account'))[0];\n                        }\n                        this.profilePic = main_account.profile_pic_url;\n                        this.accountName = main_account.name;\n                        this.accountSelectedId = main_account.id;\n                        this.params.tiktok_account_id = main_account.id;\n                        localStorage.setItem('tiktok_account', main_account.id);\n\n                        this.initialize();\n\n                    });\n            },\n            accountSelected(account) {\n                this.profilePic = account.profile_pic;\n                this.accountName = account.name;\n                this.accountSelectedId = account.id;\n                localStorage.setItem('tiktok_account', account.id);\n                this.listAccountShow = false;\n                this.params.tiktok_account_id = account.id;\n                this.initialize();\n            },\n            getRandomJobs(){\n                let jobs = this.jobs;\n                const shuffled = jobs.sort(() => 0.5 - Math.random());\n                this.jobs10 = shuffled.slice(0, 10);\n                this.$store.dispatch(COMMON_SET_SNACK, {\n                    message: 'Lấy jobs thành công !',\n                    type: \"success\"\n                });\n            },\n            capitalize(a){\n                return a.replace(/(?:^|\\s)\\S/g, function(a) { return a.toUpperCase(); });\n            },\n            remove(item) {\n                this.listSeedingTiktok.splice(this.listSeedingTiktok.indexOf(item), 1);\n                this.listSeedingTiktok = [...this.listSeedingTiktok]\n            },\n            scrollToTop() {\n                window.scrollTo(0,0);\n            },\n          onVerify(response) {\n            this.recaptcha = response;\n            this.params.recaptcha = response;\n            this.initialize();\n          },\n          resetRecaptcha: function() {\n            this.grecaptcha.reset();\n          },\n        }\n    };\n</script>\n<style scoped lang=\"scss\">\n    .work-list {\n        .job {\n            .v-avatar{\n               height: 40px !important;\n            }\n            background-color: